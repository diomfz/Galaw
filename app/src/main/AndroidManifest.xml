<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.galaw">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/galawiconlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actStress"></activity>
        <activity android:name=".actProfilePrivacy" />
        <activity android:name=".actQuotes2" />
        <activity android:name=".actQuizReport" />
        <activity android:name=".actQuiz" />
        <activity android:name=".actEditProfile" />
        <activity android:name=".actLogin" />
        <activity android:name=".actDiary" />
        <activity
            android:name=".actSignUp"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".actHome" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>